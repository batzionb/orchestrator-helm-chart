          "examples": [
            {
              "resources": {
                "requests": {
                  "memory": "64Mi",
                  "cpu": "250m"
                },
                "limits": {
                  "memory": "1Gi",
                  "cpu": "500m"
                }
              }
            }
          ]
        }
      },
      "examples": [
        {
          "devmode": false,
          "namespace": "sonataflow-infra",
          "sonataPlatform": {
            "resources": {
              "requests": {
                "memory": "64Mi",
                "cpu": "250m"
